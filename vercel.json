{
  "headers": [
    {
      "source": "/ffmpeg/(.*)\\.wasm",
      "headers": [{ "key": "Content-Type", "value": "application/wasm" }]
    },
    {
      "source": "/assets/(.*)\\.wasm",
      "headers": [{ "key": "Content-Type", "value": "application/wasm" }]
    },
    {
      "source": "/workbox-(.*)\\.js",
      "headers": [{ "key": "Cache-Control", "value": "no-cache" }]
    },
    {
      "source": "/sw\\.js",
      "headers": [{ "key": "Cache-Control", "value": "no-cache" }]
    }
  ]
}
