{
  "name": "shape",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview --strictPort --port 4173",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "format": "prettier -w .",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ci": "vitest run --reporter=verbose",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@techstark/opencv-js": "4.11.0-release.1",
    "idb": "^8.0.3",
    "marked": "^16.4.0",
    "onnxruntime-web": "1.23.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "workbox-window": "7.3.0",
    "zustand": "5.0.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "16.3.0",
    "@types/node": "24.5.2",
    "@types/react": "19.1.15",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react": "5.0.4",
    "eslint": "^9.37.0",
    "eslint-plugin-react": "^7.37.5",
    "fake-indexeddb": "^6.2.3",
    "globals": "16.4.0",
    "happy-dom": "14",
    "jsdom": "27.0.0",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.46.0",
    "vite": "5.4.20",
    "vite-plugin-pwa": "1.0.3",
    "vite-plugin-static-copy": "^3.1.3",
    "vitest": "3.2.4",
    "vercel-build": "git fetch --unshallow && git lfs fetch --all && git lfs checkout && vite build"
  }
}
